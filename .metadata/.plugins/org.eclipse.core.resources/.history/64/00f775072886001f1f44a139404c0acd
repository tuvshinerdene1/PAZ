import acm.program.GraphicsProgram;

import java.awt.Color;

import acm.graphics.*;

public class exercise6 extends GraphicsProgram {
    public void run() {
    	
    	GRect rectangle1 = new GRect (100, 100, 400, 400);
    	add(rectangle1);
    	
    	GOval circle1 = new GOval(200, 200, 200, 200);
    	add(circle1);
    }

    public static void main(String[] args) {
        new exercise6().start();
    }
}